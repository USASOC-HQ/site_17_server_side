{
  "compilerOptions": {
    "module": "none",
    "target": "es3",
    "lib": [ "es2015" ],
    "sourceMap": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": false,
    "typeRoots": [
      "../../types"
    ],
    "outDir": "../../transpiled/scripts",
    "declarationMap": false
  },
  "exclude": [
    "node_modules"
  ]
}
